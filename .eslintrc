{
  "extends": ["react-app", "prettier"],
  "plugins": ["prettier"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js"],
      "parser": "@typescript-eslint/parser"
    }
  ],
  "rules": {
    "react/jsx-wrap-multilines": "off",
    "jsx-a11y/anchor-has-content": "off",
    "quotes": ["warn", "single"],
    "semi": ["warn", "never"],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true
      }
    ]
  }
}
